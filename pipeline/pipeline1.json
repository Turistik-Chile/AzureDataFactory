				"name": "Var Error API SQL",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Copiar Datos API SQL",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "Var Error",
					"value": {
						"value": "@concat(\n  'Paso: Copiar Datos Ventas OzyTrip - Error: ',\n  replace(\n    replace(\n      replace(\n        substring(\n          activity('Copiar Datos API SQL').error.Message,\n          add(indexOf(activity('Copiar Datos API SQL').error.Message, 'Message='), 8),\n          sub(\n            indexOf(activity('Copiar Datos API SQL').error.Message, ',Source='),\n            add(indexOf(activity('Copiar Datos API SQL').error.Message, 'Message='), 8)\n          )\n        ),\n        '\\n', ' '\n      ),\n      '\\r', ' '\n    ),\n    '\"', ''\n  )\n)\n",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Enviar Error Copiar Datos",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "Var Error API SQL",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "POST",
					"url": "https://prod-14.chilecentral.logic.azure.com:443/workflows/9e3a00ca28cf4a4db3c001a96f107a6b/triggers/When_an_HTTP_request_is_received/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2FWhen_an_HTTP_request_is_received%2Frun&sv=1.0&sig=vEoXPLtmcD5b4D6KJTAQSkh_sfe86WEmmPsgAjq4J10",
					"body": {
						"value": "{\n    \"message\" : \"@{replace(replace(replace(variables('Var Error'), '\\n', ' '), '\\r', ' '), '\\\"', '')}\",\n    \"dataFactoryName\" : \"@{pipeline().DataFactory}\", \n    \"pipelineName\" : \"@{pipeline().Pipeline}\",\n    \"MensajeError\": \"@{variables('Var Error')}\",\n    \"Modulo\":\"@{pipeline().parameters.p_modulo}\",\n    \"Hora\":\"@{formatDateTime(addHours(utcNow(), -3), 'yyyy-MM-dd HH:mm:ss')}\"\n   }",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Var Error Token",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Obtener token",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "Var Error",
					"value": {
						"value": "@concat(\n  'Paso: Obtener Token - Error: ',\n  coalesce(\n    string(activity('Obtener token').output.error),\n    string(activity('Obtener token').Error.ErrorCode)\n  )\n)\n",
						"type": "Expression"
					}
				}
			},
